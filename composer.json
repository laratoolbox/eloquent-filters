{
    "name": "laratoolbox/eloquent-filters",
    "description": "Easily add filters into eloquent models",
    "keywords": [
        "laratoolbox",
        "laravel",
        "eloquent",
        "filters"
    ],
    "homepage": "https://github.com/laratoolbox/eloquent-filters",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Semih ERDOGAN",
            "email": "hasansemiherdogan@gmail.com",
            "homepage": "https://semiherdogan.net/",
            "role": "Developer"
        },
        {
            "name": "Dincer DEMIRCIOGLU",
            "email": "dinncer@gmail.com",
            "homepage": "https://dincer.info/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/support": "^5.5|^5.6|^5.7|^5.8|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.5|^3.6|^3.7|^3.8|^4.0|^5.0|^6.0",
        "phpunit/phpunit": "~6.0|^7.0|^7.5|^8.4|^9.0"
    },
    "autoload": {
        "psr-4": {
            "LaraToolbox\\EloquentFilters\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaraToolbox\\EloquentFilters\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaraToolbox\\EloquentFilters\\PackageServiceProvider"
            ]
        }
    }
}
